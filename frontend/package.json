    "chart.js": "^2.7.3",
    "history": "^4.7.2",
    "jquery": "^1.9.1",
    "node-sass": "^4.10.0",
    "nodejs": "0.0.0",
    "npm": "^6.5.0",
    "react": "^16.6.3",
    "react-bootstrap": "^0.32.4",
    "react-chartjs": "^1.2.0",
    "react-chartjs-2": "^2.7.4",
    "react-d3-radar": "^1.0.0-rc6",
    "react-dom": "^16.6.3",
    "react-file-base64": "^1.0.3",
    "react-file-reader": "^1.1.4",
    "react-redux": "^5.1.1",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8",
    "react-spinners": "^0.5.1",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.1",
    "start": "^5.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint src --ext js,jsx",
    "lint-jsx": "eslint src --ext jsx",
    "build-preprod": "npm run build && rm ./build/static/**/*.map && rm -rf ../backend/public/* && mv ./build/* ../backend/public/",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "browser": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "linebreak-style": 0,
      "react/jsx-filename-extension": [
        "js",
        "jsx"
      ],
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/mouse-events-have-key-events": "off",
      "react/prop-types": "off"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "husky": "^1.2.0",
    "prettier-eslint": "^8.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "description": ""
}
